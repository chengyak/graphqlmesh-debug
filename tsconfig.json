{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "strictNullChecks": false,
      "skipLibCheck": true,
      "types": ["node"],
      "allowJs": true
    },
    "exclude": ["node_modules", "dist"]
  }
  